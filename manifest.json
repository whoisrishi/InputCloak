{
    "name": "InputClock",
    "version": "1.0",
    "description": "Extension that will cover your input field",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
